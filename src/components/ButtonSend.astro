---
    interface Props {
        etiqueta: string;
    }

    const etiqueta = Astro.props.etiqueta;
---
<button class="boton-envio">
    {etiqueta}
</button>
<style>
    .boton-envio{

        background-color: rgb(38, 95, 250);
        color:white;
        width: 100px;
        height: 40px;
        border-radius: 10px;
        border-color: rgb(255, 255, 255);
        box-shadow: 2px 2px 2px 0px rgb(187, 186, 186);
    }
</style>